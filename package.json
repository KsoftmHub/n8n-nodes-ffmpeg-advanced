{
  "name": "@ksoftm/n8n-nodes-ffmpeg-advanced",
  "version": "1.0.0",
  "description": "Advanced audiovisual processing node for n8n based on FFmpeg research",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "ffmpeg",
    "video",
    "audio",
    "transcode"
  ],
  "license": "MIT",
  "author": "Ksoftm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KsoftmHub/n8n-nodes-ffmpeg-advanced.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/FFmpeg.node.js",
  "n8n": {
    "nodes": [
      "dist/FFmpeg.node.js"
    ]
  },
  "scripts": {
    "build": "node bin/build.js",
    "dev": "npm run build -- --watch",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "uuid": "^9.0.0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/node": "^14.14.31",
    "@types/uuid": "^9.0.1",
    "n8n-workflow": "^0.117.0",
    "n8n-core": "^0.117.0",
    "typescript": "^4.1.3",
    "esbuild": "^0.27.1",
    "semantic-release": "^24.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/changelog": "^6.0.3"
  },
  "release": {
    "branches": [
      "master",
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  }
}
